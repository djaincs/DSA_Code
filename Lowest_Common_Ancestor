#include<bits/stdc++.h>
using namespace std;

class Node{
	public:
		int data;
		Node* left;
		Node* right;
		
		Node(int val){
			data = val;
			left = NULL;
			right = NULL;
		}
};

Node* LCA(Node* root, Node* a,Node* b){
	if(root == NULL || root== a || root == b)
		return root;
	Node* lft = LCA(root->left,a,b);
	Node* rht = LCA(root->right,a,b);
	
	if(lft == NULL)
		return rht;
	if(rht == NULL)
		return lft;
	return root;			
}

int main(){
	
	Node* root = new Node(1);
	
	root->left = new Node(2);
	root->right = new Node(3);
	
	root->left->left = new Node(4);
	root->left->right = new Node(5);
	root->right->left = new Node(6);
	root->right->right = new Node(7);
	
	
	Node* ans = LCA(root,root->left,root->right);
	cout<<ans<<endl;	
	return 0;
}
